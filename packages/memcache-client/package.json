{
  "name": "memcache-client",
  "version": "2.0.0",
  "description": "NodeJS memcached client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "coverage": "xrun xarc/test-cov",
    "lint": "xrun xarc/lint",
    "test": "npm run lint && xrun xarc/test-only",
    "playground": "ts-node-dev ./src/playground.ts",
    "testp": "MEMCACHED_SERVER=10.1.1.1:11211 npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/memcache.git"
  },
  "keywords": [
    "memcache",
    "memcached",
    "nodejs",
    "client",
    "ascii",
    "protocol"
  ],
  "author": "Joel Chen <joel123@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "lodash.defaults": "4.2.0",
    "memcache-parser": "file:../memcache-parser",
    "optional-require": "1.1.8",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@types/lodash.defaults": "4.2.7",
    "lodash": "4.17.21",
    "memcached-njs": "^1.0.0",
    "source-map-support": "0.5.19",
    "zstd.ts": "1.1.3"
  },
  "files": [
    "dist"
  ],
  "@xarc/module-dev": {
    "features": [
      "eslint",
      "eslintTS",
      "jest",
      "jestTS",
      "prettier",
      "typedoc",
      "typescript"
    ]
  }
}
